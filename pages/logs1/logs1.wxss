/* pages/logs1/logs1.wxss */

page {
  height: 100%;
  width: 100%;
  overflow: hidden;
}

page .page {
  height: 100%;
  width: 100%;
}

.page .back {
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0;
  z-index: -1;
}

.view {
  position: relative;
}

.page .view {
  width: 80%;
  height: 90rpx;
  margin-left: 10%;
  margin-bottom: 20rpx;
  border-bottom: 1px solid #ccc;
  line-height: 90rpx;
}

.view  input {
  background: none;
  color: #fff;
  padding-top: 32rpx;
  padding-left: 60rpx
}

.view image {
  height: 40rpx;
  width: 40rpx;
  position: absolute;
  top: 32rpx;
}

.top {
  margin-top: 10%;
}

.lg {
  height: 180rpx;
  width: 180rpx;
  background: none;
  border-radius: 50%;
  overflow: hidden;
  margin: 0 auto;
  margin-top: 200rpx;
}

.lg image {
  height: 100%;
  width: 100%;
}

button {
  width: 80%;
  margin-top: 100rpx;
  background: #eab617;
  color: #fff;
}

.text {
  width: 100%;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  margin-top: 12rpx;
}
